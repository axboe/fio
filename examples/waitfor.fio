[global]
threads=1
group_reporting
filename=/tmp/data
filesize=128m

[writers]
rw=write
bs=128k
numjobs=4
runtime=10

[readers]
new_group
wait_for=writers
rw=randread
bs=4k
numjobs=4
runtime=10

[writers2]
new_group
wait_for=readers
rw=randwrite
bs=4k
numjobs=4
runtime=10

[readers2]
new_group
wait_for=writers2
rw=randread
bs=4k
numjobs=4
runtime=10
