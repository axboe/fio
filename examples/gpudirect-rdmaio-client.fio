# Example gpudirect rdma client job
[global]
ioengine=rdma
hostname=${RDMA_SERVER}
port=8888
bs=1m
size=100g
mem=cudamalloc
gpu_dev_id=0

[cudadirect-client-rdma-write]
# use IBV_WC_RDMA_WRITE
rw=write
direct=1
numjobs=2
iodepth=16

[cudadirect-client-rdma-read]
# use IBV_WC_RDMA_READ
rw=read
direct=1
numjobs=2
iodepth=16
