# Run e4defrag and aio-dio workers in parallel
[e4defrag]
stonewall
time_based=30
runtime=30
ioengine=e4defrag
buffered=0
donorname=file.def
filename=file1
inplace=0
rw=write

[random-aio-32k]
ioengine=libaio
